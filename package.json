{
  "name": "trySciunit",
  "version": "1.0.0",
  "description": "A responsive web-based tutorial for sciunit-cli",
  "main": "dist/backend/server.js",
  "scripts": {
    "build": "./tasks/build.sh",
    "build-tests": "./tasks/build-tests.sh",
    "d": "npm run build && node dist/backend/server",
    "electron": "npm run build && electron dist/electron",
    "test": "mocha src/**/*-tests.js"
  },
  "config": {
    "data-cover-never": [
      "node_modules",
      "test",
      "src/**/*-tests.js"
    ]
  },
  "author": "Qili Sui",
  "devDependencies": {
    "babel-preset-es2105": "0.0.0",
    "babel-preset-react": "6.11.1",
    "babel-register": "6.9.0",
    "babelify": "7.3.0",
    "browserify": "13.0.1",
    "chai": "3.5.0",
    "electron-prebuilt": "1.2.5",
    "jsdom": "9.4.1",
    "mocha": "2.5.3",
    "node-sass": "3.8.0",
    "sinon": "1.17.4"
  },
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "express": "4.14.0",
    "immutable": "3.8.1",
    "path": "^0.12.7",
    "react": "15.2.0",
    "react-addons-css-transition-group": "^15.6.2",
    "react-blink": "^1.0.3",
    "react-dom": "15.2.0",
    "react-redux": "4.4.5",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^4.0.8",
    "redux": "3.5.2",
    "redux-thunk": "2.1.0",
    "shelljs": "^0.7.8",
    "socket.io": "^2.0.4",
    "utf-8-validate": "^3.0.4",
    "webpack": "^3.5.2",
    "ws": "1.1.1"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ]
    ]
  }
}
